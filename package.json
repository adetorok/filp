{
  "name": "homeflip-software",
  "version": "1.0.0",
  "description": "Complete fix-and-flip property management software",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "jest",
    "test:api": "jest server/tests/",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "node server/seed.js",
    "db:reset": "npx prisma migrate reset --force",
    "db:studio": "npx prisma studio",
    "start:prod": "NODE_ENV=production node server/index.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "docker:build": "docker build -t homeflip-api .",
    "docker:run": "docker run -p 5000:5000 homeflip-api"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "prisma": "^6.17.1",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "real-estate",
    "property-management",
    "investment",
    "homeflip"
  ],
  "author": "HomeFlip Software",
  "license": "MIT"
}
